{
  "name": "honobackend-withoutopenspec",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --minify",
    "lint": "biome lint .",
    "lint:check": "biome check .",
    "lint:fix": "biome format --write",
    "test": "cross-env NODE_ENV=test vitest",
    "typecheck": "tsc --noEmit",
    "dev:db": "turso dev --db-file dev.db",
    "generate:betterauth": "bunx @better-auth/cli generate --config /src/lib/auth-cli.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:migrate:local": "wrangler d1 migrations apply honowithbetterauth --local",
    "db:migrate:prod": "wrangler d1 migrations apply honowithbetterauth"
  },
  "dependencies": {
    "better-auth": "^1.3.9",
    "dotenv": "^17.2.2",
    "dotenv-expand": "^12.0.3",
    "drizzle-orm": "^0.44.5",
    "drizzle-zod": "^0.8.3",
    "hono": "^4.9.6",
    "hono-pino": "^0.10.2",
    "pino": "^9.9.4",
    "pino-pretty": "^13.1.1",
    "stoker": "^2.0.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.9",
    "@biomejs/biome": "^2.2.3",
    "@cloudflare/workers-types": "^4.20250909.0",
    "@types/node": "^24.3.1",
    "cross-env": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2",
    "wrangler": "^4.35.0"
  }
}